<profile>
    <message id="msg_id">
        <type>data</type>
        <pointsource>SIGFOX</pointsource>
        <tag_pattern>SIGFOX-IOT.AGL.{source}_{device_type}.{device}.{point}</tag_pattern>
        <time_style>unix</time_style>
        <point>
            <item>battery</item>
            <type>Float32</type>
			<engunits>%</engunits>
        </point>
        <point>
            <item>signallevel</item>
            <alias>SIGNAL</alias>
            <type>Float32</type>
			<engunits>dB</engunits>
        </point>
        <point>
            <item>alertasserted</item>
            <alias>ALERT</alias>
            <type>digital</type>
            <digitalset>OffOn</digitalset>
        </point>
        <point>
            <item>schedulerenabled</item>
            <alias>rescheduled</alias>
            <type>digital</type>
            <digitalset>OffOn</digitalset>
        </point>
        <point>
            <item>measurementinterval</item>
            <alias>INTERVAL</alias>
            <type>Float32</type>
       </point>
        <point>
            <item>transmissioninterval</item>
            <alias>TX_INTERVAL</alias>
            <type>Float32</type>
        </point>
		<series>
			<name>data</name>
			<point>
				<item>rawDistance</item>
				<alias>LEVEL_RAW</alias>
				<type>Float32</type>
				<engunits>mm</engunits>
			</point>
			<point>
				<item>waterLevel</item>
				<alias>LEVEL</alias>
				<type>Float32</type>
				<engunits>mm</engunits>
			</point>
		</series>
    </message>
</profile>
